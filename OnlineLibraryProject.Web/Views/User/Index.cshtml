    <link href="~/PhotoFolio/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/PhotoFolio/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">
