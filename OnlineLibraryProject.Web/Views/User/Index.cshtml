<body>

            <!-- ======= Header ======= -->
    @await Html.PartialAsync("UpHeaderPartial")
    @model List<Book>

            <!-- End Header -->
    <!-- ======= Hero Section ======= -->
    <!-- End Hero Section -->

    

        <!-- ======= Gallery Section ======= -->
        <section id="hero" class="hero d-flex flex-column justify-content-center align-items-center" data-aos="fade" data-aos-delay="1500">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-6 text-center">
                        <h2>I'm <span>Jenny Wilson</span> a Professional Photographer from New York City</h2>
                        <p>Blanditiis praesentium aliquam illum tempore incidunt debitis dolorem magni est deserunt sed qui libero. Qui voluptas amet.</p>
                        <a href="contact.html" class="btn-get-started">Available for hire</a>
                    </div>
                </div>
            </div>
        </section><!-- End Hero Section -->


    <main id="main" data-aos="fade" data-aos-delay="1500">
            <!-- ======= Gallery Section ======= -->
            <section id="gallery" class="gallery">
                <div class="container-fluid">
                    <div class="row gy-4 justify-content-center">

                        <div class="col-xl-4 col-lg-4 col-md-6">
                            <div class="gallery-item h-100">
                                <img src="~/PhotoFolio/assets/img/gallery/love.jpg" class="img-fluid" alt="">
                                <div class="gallery-links d-flex align-items-center justify-content-center">
                                    <h2 style="color:pink"><i>Love</i> </h2>
                                </div>
                            </div>
                        </div><!-- End Gallery Item -->

                        <div class="col-xl-4 col-lg-4 col-md-6">
                            <div class="gallery-item h-100">
                                <img src="~/PhotoFolio/assets/img/gallery/history.jpg" class="img-fluid" alt="">
                                <div class="gallery-links d-flex align-items-center justify-content-center">
                                    <h2 style="color:saddlebrown"><i>History</i> </h2>
                                </div>
                            </div>
                        </div><!-- End Gallery Item -->

                        <div class="col-xl-4 col-lg-4 col-md-6">
                            <div class="gallery-item h-100">
                                <img src="~/PhotoFolio/assets/img/gallery/science fiction.jpg" class="img-fluid" alt="">
                                <div class="gallery-links d-flex align-items-center justify-content-center">
                                    <h2 style="color:darkred"><i>Science Fiction</i> </h2>
                                </div>
                            </div>
                        </div><!-- End Gallery Item -->

                        <div class="col-xl-4 col-lg-4 col-md-6">
                            <div class="gallery-item h-100">
                                <img src="~/PhotoFolio/assets/img/gallery/child.jpg" class="img-fluid" alt="">
                                <div class="gallery-links d-flex align-items-center justify-content-center">
                                    <h2 style="color:dodgerblue"><i>Child</i> </h2>
                                </div>
                            </div>
                        </div><!-- End Gallery Item -->

                        <div class="col-xl-4 col-lg-4 col-md-6">
                            <div class="gallery-item h-100">
                                <img src="~/PhotoFolio/assets/img/gallery/adventure.jpg" class="img-fluid" alt="">
                                <div class="gallery-links d-flex align-items-center justify-content-center">
                                    <h2 style="color:forestgreen"><i>Adventure</i> </h2>
                                </div>
                            </div>
                        </div><!-- End Gallery Item -->

                        <div class="col-xl-4 col-lg-4 col-md-6">
                            <div class="gallery-item h-100">
                                <img src="~/PhotoFolio/assets/img/gallery/classics.jpg" class="img-fluid" alt="">
                                <div class="gallery-links d-flex align-items-center justify-content-center">
                                    <h2 style="color:slategrey"><i>Clasics</i> </h2>
                                </div>
                            </div>
                        </div><!-- End Gallery Item -->

                    </div>                   
                </div>
            </section><!-- End Gallery Section -->

    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    @await Html.PartialAsync("FooterPartial")

    <a href="#" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <div id="preloader">
        <div class="line"></div>
    </div>

    <!-- Vendor JS Files -->
    <script src="~/PhotoFolio/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/PhotoFolio/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/PhotoFolio/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/PhotoFolio/assets/vendor/aos/aos.js"></script>
    <script src="~/PhotoFolio/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/PhotoFolio/assets/js/main.js"></script>

</body>

</html>


